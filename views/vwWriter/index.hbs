<div class="card">
  <h4 class="card-header">
    List
  </h4>
   <br>
    {{#if this.empty}}
    <h5 class="card-title">No data!</h5>
    {{else}}
      {{#each this.allpaper}}
      
        <div class="card mb-3" >
          <div class="row no-gutters" style="padding: 1%;">
            <div class="col-md-4" style="padding-right: 2%;">
              <img src="{{LinkAnhDD}}" alt="..." style="width:100%; height: 210px;" class="mx-auto d-block">
            </div>
            <div class="col-md-8">
            
                <h5 class="card-title">{{TieuDe}}</h5>
                <p class="card-text">{{TomTat}}</p>
                

              <div style=" position: absolute;bottom:0px;right:0px">
              <a class="btn btn-outline-success btn-sm"  href="../../../news/forAEW_0w?id={{MaBV}}" role="button" title="View">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye-fill" viewBox="0 0 16 16">
                      <path d="M10.5 8a2.5 2.5 0 1 1-5 0 2.5 2.5 0 0 1 5 0z"/>
                      <path d="M0 8s3-5.5 8-5.5S16 8 16 8s-3 5.5-8 5.5S0 8 0 8zm8 3.5a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7z"/>
                    </svg>
                </a>
                </div>
                <p class="card-text" style=" position: absolute;bottom:0px;"> <small class="text-muted">
                  {{TenCM}}
                  </small></p>
            
            </div>
          </div>
        </div>

      {{/each}}
    </div>
    {{/if}}

</div>



